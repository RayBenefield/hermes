{
    "presets": [
        "es2015",
        "es2016",
        "es2017"
    ],
    "plugins": [
        [
            "wildcard",
        {
            "exts": [
                "js",
            "es6",
            "es",
            "jsx",
            "javascript",
            "json"
            ],
            "noCamelCase": true
        }
        ],
        "transform-object-rest-spread"
    ]
}
